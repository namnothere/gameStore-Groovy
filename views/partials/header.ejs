                  <li>
                    <a href="/user/signin">
                      <i class="fas fa-sign-in-alt u-s-m-r-9"></i>
                      Sign in</a
                    >
                  </li>
                  <li>
                    <a href="/user/signup">
                      <i class="fas fa-sign-in-alt u-s-m-r-9"></i>
                      Sign up</a
                    >
                  </li>

                  <% } %>
                  
                  <!-- if user is logged in then create a logout button -->
                  <% if (typeof user !== 'undefined') { %>
                    <li>
                      <a href="/user/profile">
                        <i class="fas fa-sign-in-alt u-s-m-r-9"></i>
                        Profile</a
                      >
                    </li>
                    <li>
                      <a href="/user/logout">
