      <!-- Bottom-Header -->
      <div class="full-layer-bottom-header">
        <div class="container">
          <div class="row align-items-center">
            <div class="col-lg-3">
              <div class="v-menu v-close">
                <span class="v-title">
                  <i class="ion ion-md-menu"></i>
                  All Categories
                  <i class="fas fa-angle-down"></i>
                </span>
                <nav>
                  <div class="v-wrapper">
                    <ul class="v-list animated fadeIn">
                      <li <% if (pageName == "All Products") { %>
                        class="nav-item active"
                        <%} else {%>
                          class="nav-item"
                        <%} %> 
                      >
                        <a class="nav-link" href="/products/">All</a>
                      </li>
                      <!-- Limit to 6 most popular categories -->

                      <% if (categories) { %>
                        <% categories = categories.slice(0, 7) %>
                        <% categories.forEach( category => { %>
                          <li <% if (pageName === category.title) { %>
                            class="nav-item active"
                            <% } else { %>
                              class="nav-item"
                            <% } %> 
                          >
                        <a href="/products/<%= category.slug %>">
                          <%= category.title %>
                        </a>
                      </li>
                      <% }) %>
                    <% } %>
                    </ul>
                  </div>
                </nav>
              </div>
            </div>
            <div class="col-lg-9">
              <ul class="bottom-nav g-nav u-d-none-lg">
                <!-- <li>
                  <a href="custom-deal-page.html"
                    >New Arrivals
                  </a>
                </li> -->
                <% if (categories) { %>
                  <% categories = categories.slice(0, 3) %>
                  <% categories.forEach( category => { %>
                    <li <% if (pageName === category.title) { %>
                      class="nav-item active"
                      <% } else { %>
                        class="nav-item"
                      <% } %> 
                    >
                  <a href="/products/<%= category.slug %>">
                    <%= category.title %>
                  </a>
                </li>
                <% }) %>
              <% } %>

              </ul>
            </div>
          </div>
        </div>
